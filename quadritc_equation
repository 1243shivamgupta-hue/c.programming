// Online C compiler to run C program online
#include <stdio.h>
#include<math.h>
#include<stdlib.h>
int main() {
    float a,b,c,d,root1,root2,real,img;
    printf("Enter three exponention : ");
    scanf("%f%f%f",&a,&b,&c);
    if(a==0){
        printf("Enter the value again");
        exit(0);
    }
    d=(b*b)-(4*a*c);
    if(d>0){
        root1 = (-b + sqrt(d)) / (2 * a);
        root2 = (-b - sqrt(d)) / (2 * a);
        printf("its real number & disinct");
        printf("root1=%f\troot2=%f",root1,root2);
    }
    
    else if (d==0){
        root1=-b/(2*a);
        root2=root1;
        printf("its real number & equal");
        printf("root1=%f\troot2=%f",root1,root2);
    }
    else{
        real=-b/(2*a);
        img=sqrt(-d)/(2*a);
        printf("its real number & imag");
        printf("root1=%f+i%f",real,img);
        printf("root2=%f-i%f",real,img);
    }


    return 0;
}
